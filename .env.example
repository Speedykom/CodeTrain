# CodeTrain Configuration
# Copy this file to .env and fill in your values

# OpenRouter API Key (required for LLM features)
# Get your key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=your-openrouter-api-key-here

# OpenRouter Configuration (optional - defaults shown)
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_MODEL=moonshotai/kimi-k2.5 # <-- you can change this to any model you like

# Optional: For OpenRouter rankings
OPENROUTER_HTTP_REFERRER=https://github.com/Speedykom/CodeTrain
OPENROUTER_SITE_NAME=CodeTrain
